  "scripts": {
    "build": "next build",
    "build-storybook": "build-storybook",
    "dev": "next dev -p 4000",
    "format": "prettier --write \"**/*.+(js|jsx|json|yml|yaml|css|less|scss|ts|tsx|md|graphql|mdx)\"",
    "lint": "eslint --cache --ext .js,.ts,.tsx ./ --fix",
    "lint:css": "stylelint './**/*.js' './**/*.jsx' './**/*.tsx' './**/*.ts'",
    "start": "next start -p 4000",
    "storybook": "start-storybook -p 6006",
    "test": "jest",
    "tailwind": "npx tailwindcss -i ./src/styles/styles.css -o ./src/styles/app.css --watch",
    "type-check": "tsc",
    "validate": "npm-run-all --parallel lint lint:css format type-check test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
